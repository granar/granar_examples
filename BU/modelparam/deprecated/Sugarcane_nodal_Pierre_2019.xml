<?xml version="1.0" encoding="utf-8"?>

<granar>
	<!-- Credit : Annelie Marquardt, Anne Rae, and Johann Pierre. 2019 -->
	
	<!-- General parameters -->
	<planttype param="1" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="1" />
	
	<!-- Cell layers -->
	<stele cell_diameter="0.027" n_layers="1" layer_diameter = "0.585" order="1"/>
	<pericycle cell_diameter="0.029" n_layers="1"  order="2"/>
	<endodermis cell_diameter="0.029" n_layers="1"  order="3"/>
	<cortex cell_diameter="0.037" n_layers="11"  order="4"/>
	<exodermis cell_diameter="0.026" n_layers="2" order="5"/>	
	<epidermis cell_diameter="0.027" n_layers="1" order="6"/>


	<!-- Vessels-->
	<xylem max_size="0.0951" n_files="9" order="1.5" ratio ="2.3" />
	<phloem max_size="0.01" n_files="3" />
	
	<!-- Other cell types -->
	<!--aerenchyma proportion="0" n_files="10"/-->
	<aerenchyma proportion="0.0" n_files="10"/>

</granar>