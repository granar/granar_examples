<?xml version="1.0" encoding="utf-8"?>

<granar>
	
	<!-- General parameters -->
	<planttype param="2" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="3" />
	
	<!-- Cell layers -->
	<stele cell_diameter="0.009" n_layers="1" layer_diameter = "0.136" order="1"/>
	<pericycle cell_diameter="0.0065" n_layers="1"  order="2"/>
	<endodermis cell_diameter="0.008" n_layers="1"  order="3"/>
	<cortex cell_diameter="0.0315" n_layers="21"  order="4"/>
	<exodermis cell_diameter="0.022" n_layers="1" order="5"/>	
	<epidermis cell_diameter="0.01" n_layers="1" order="6"/>


	<!-- Vessels-->
	<xylem max_size="0.03" n_files="3" order="1.5" ratio ="2.75" />
	<phloem max_size="0.01" n_files="3" />
	
	<!-- Other cell types -->
	<!--aerenchyma proportion="0" n_files="10"/-->
	<aerenchyma proportion="0" n_files="10"/>

</granar>