<?xml version="1.0" encoding="utf-8"?>

<granar>
	
	<!-- General parameters -->
	<planttype param="1" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="2" />
	
	<!-- Cell layers -->
	<stele cell_diameter="0.018" n_layers="1" layer_diameter = "0.62" order="1"/>
	<pericycle cell_diameter="0.014" n_layers="1"  order="2"/>
	<endodermis cell_diameter="0.014" n_layers="1"  order="3"/>
	<cortex cell_diameter="0.049" n_layers="8"  order="4"/>
	<exodermis cell_diameter="0.035" n_layers="1" order="5"/>	
	<epidermis cell_diameter="0.029" n_layers="1" order="6"/>


	<!-- Vessels-->
	<xylem max_size="0.120" n_files="6" order="1.5" ratio ="1.86" />
	<phloem max_size="0.01" n_files="3" />
	
	<!-- Other cell types -->
	<!--aerenchyma proportion="0" n_files="10"/-->
	<aerenchyma proportion="0" n_files="10"/>

</granar>